#:kivy 2.3.0
<CekGaji>:
    BoxLayout:
        orientation: 'vertical'
        
        MDLabel:
            text: "Cek Data Gaji Karyawan"
            halign: 'center'
            font_style: 'H5'
            size_hint_y: None
            height: dp(50)
        
        BoxLayout:
            id: table_container
            padding: dp(10)
        
        MDRaisedButton:
            text: "Kembali"
            pos_hint: {'center_x': 0.5}
            on_release: app.root.current = 'main'

        MDRectangleFlatButton:
            text: "Cetak PDF"
            on_release: root.print_pdf()
